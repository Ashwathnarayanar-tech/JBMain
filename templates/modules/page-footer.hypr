<footer class="mz-pagefooter"> 
<style>

#footer-email-signup-button, #footer-sms-signup-button {
  height: 30px;
  padding: 4px 14px;
  position: relative;
  width: 120px;
  background: #bd5709 !important;
  border: 1px solid #E28104;
  color: #fff;
  cursor: pointer;
  font-family: arial;
  font-size: 14px;
  font-weight: bold;
  text-decoration: none;
  text-transform: uppercase;
  border-radius: 0;
  border: solid 1px #da7c0c;
  background: #bd5709;
  background: -webkit-gradient(linear, left top, left bottom, from(#f88e11), to(#f06015));
  background: -moz-linear-gradient(top, #f88e11, #f06015);

}
#footer-email-signup-msg, #footer-sms-signup-msg
{
  display: inline-block;
  vertical-align: left;
}
</style>

<div class="mz-footer-row"> 
 <div class="mz-footer-row2">
	{% if pageContext.cmsContext.page.path != 'subscribe' %}
		{% include "modules/email-sms-signup" %}
	{% endif %}
  </div>
</div>
    <div class="mz-footer-row"> 
        {% block mz-footer-row2 %}
            <div class="mz-mobile jblogo"></div>
        <div class="mz-footer-row2">
            <div class="mz-wrapper">
                <!-- FOOTER 1ST COL -->   
                    <div class="mz-column mz-column-1">
                      <div class="mz-desktop">
                        <span class="mz-footer-title">
                          <a href="/CustomerService">Customer Service</a>
                        </span>
                      </div>
                      <div class="mz-mobile">
                        <span class="mz-footer-title footer-title-click" aria-expanded="false">
                          <a href="/CustomerService">Customer Service</a>
                          <img class="footer-plus" src="/resources/images/icons/plus_footer.png" alt="footer-plus" />
                          <img class="footer-minus" src="/resources/images/icons/minus_footer.png" alt="footer-minus" />
                        </span>
                      </div>
                      <ul class="mz-footer-links">
                        {% dropzone "dropzone_area_footer_2" scope="site" %}
                      </ul>
                      <div class="mz-desktop">
                        <span style="margin-top: 32px" class="mz-footer-title">
                          <a href="/online-order-info-index">Help with Ordering</a>
                        </span>
                      </div>
                      <div class="mz-mobile">
                        <span class="mz-footer-title footer-title-click" aria-expanded="false">
                          <a href="/online-order-info-index">Help with Ordering</a>
                          <img class="footer-plus" src="/resources/images/icons/plus_footer.png" alt="footer-plus" />
                          <img class="footer-minus" src="/resources/images/icons/minus_footer.png" alt="footer-minus" />
                        </span>
                      </div>
                      <ul class="mz-footer-links">
                        {% dropzone "dropzone_area_footer_5" scope="site" %}
                      </ul>
                    </div>
                    <!-- FOOTER 2ND COL -->
    				<div class="mz-column mz-column-2">
              <div class="mz-desktop">
                <span class="mz-footer-title">
                  <a href="/aboutjellybelly">
                    About Jelly Belly
                  </a>
                </span>
              </div>
              <div class="mz-mobile">
                <span class="mz-footer-title footer-title-click" aria-expanded="false">
                  <a href="/aboutjellybelly">About Jelly Belly</a>
                  <img class="footer-plus" src="/resources/images/icons/plus_footer.png" alt="footer-plus" />
                  <img class="footer-minus" src="/resources/images/icons/minus_footer.png" alt="footer-minus"/>
                </span>
              </div>
              <ul class="mz-footer-links mz-desktop">
                {% dropzone "dropzone_area_footer_1" scope="site" %}
              </ul>
            </div>
				    <!-- FOOTER 3RD COL --> 
    				<div class="mz-column mz-column-3">
    					<div class="mz-desktop">
    						<span class="mz-footer-title">
    						    <a href="/newsandevents">News &amp; Events</a>
    						</span>
    					</div>
    					<div class="mz-mobile">
    						<span class="mz-footer-title footer-title-click" aria-expanded="false">
    					        <a href="/newsandevents">News &amp; Events</a>
                      <img class="footer-plus" src="/resources/images/icons/plus_footer.png" alt="footer-plus" />
                      <img class="footer-minus" src="/resources/images/icons/minus_footer.png" alt="footer-minus" />
    					    </span> 
    					</div>
    					<ul class="mz-footer-links mz-desktop">
    					{% dropzone "dropzone_area_footer_3" scope="site" %}
    					</ul>
              <div class="mz-desktop">
                <span class="mz-footer-title" style="margin: 38px 0 0px 0">Quick Links</span>
              </div>  
              <div class="mz-mobile">
                <span class="mz-footer-title footer-title-click" aria-expanded="false">Quick Links
                  <img class="footer-plus" src="/resources/images/icons/plus_footer.png" alt="footer-plus" />
                  <img class="footer-minus" src="/resources/images/icons/minus_footer.png" alt="footer-minus" />
                </span>
              </div>  
              <ul class="mz-footer-links mz-desktop">
              {% dropzone "dropzone_area_footer_4" scope="site" %}
              </ul>
    				</div>
    				<!-- FOOTER 4TH COL -->
    				<div class="mz-column mz-column-4">
    					<div class="jb-follow-us">
                <span>Follow Us on</span>
                <ul>
                    <li>
                        <a style="font-size:30px" href="https://www.pinterest.com/jellybelly" target="_blank" title="Jelly Belly on Pinterest" class="link-footer-pinterest"></a>
                    </li>
                    <li>
                        <a style="font-size:30px" href="https://www.facebook.com/jellybelly" target="_blank" title="Jelly Belly on Facebook" class="link-footer-facebook"></a>
                    </li>
                    <li>
                        <a style="font-size:30px" href="https://www.youtube.com/jellybelly" target="_blank" title="Jelly Belly on Youtube" class="link-footer-youtube"></a>
                    </li>
                    <li>
                        <a style="font-size:30px" href="https://instagram.com/JellyBellyCandyCo" target="_blank" title="Jelly Belly on Instagram" class="link-footer-instagram"></a>
                    </li>
                    <li>
                        <a style="font-size:30px" href="https://www.linkedin.com/company/jelly-belly-candy-company" target="_blank" title="Jelly Belly on Linked In" class="link-footer-linkedin"></a>
                    </li>
                </ul>
                <a class="free-tour-box" href="/visitjellybelly">
                    <span>Free Tours</span>
                    <img class="tour-icon-footer" width="25" src="data:image/png;base64," alt="" />
                    <img class="tour-icon-footer-white" width="25" src="data:image/png;base64," alt="" />
                </a>
              </div>
    				</div>
					{% if pageContext.isTablet %}
						<div style="width: 100%; margin-left: auto; margin-right: auto;clear:both"> 
              <span class="sweet-rewards-label">Sweet Rewards</span>
							<a style="display: block;text-align: center;" href="/international" aria-label="Jelly Belly International Sites" title="Jelly Belly International Sites"> 
								<img class="new-lazy-load" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-src="//cdn-tp1.mozu.com/9046-m1/cms/files/b4c9d78e-047f-49fb-874b-e6bfe0e76985" style="max-width:100%" alt="Jelly Belly International Sites"></img>
							</a>
						</div>
					{% endif %}					
					
					{% if pageContext.isMobile %}
						<div style="width: 100%; margin-left: auto; margin-right: auto; clear:both;"> 
              <span class="sweet-rewards-label">Sweet Rewards</span>
							<center>
							<a href="/international" aria-label="Jelly Belly International Sites" title="Jelly Belly International Sites"> 
								<img class="new-lazy-load" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" 
								data-src="{% make_url "image" "//cdn-tp1.mozu.com/9046-m1/cms/files/international-sites-mobile-5.png" with max=341 %}" style="max-width:100%" alt="Jelly Belly International Sites"></img>
							</a>
							</center>
						</div>
					{% endif %}

					
					{% if pageContext.isDesktop %}
						<div style="clear:both;"> 
              <span class="sweet-rewards-label">Sweet Rewards</span>
							<a href="/international" aria-label="Jelly Belly International Sites" title="Jelly Belly International Sites"> 
								<img class="new-lazy-load footer-bottom-flag" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-src="//cdn-tp1.mozu.com/9046-m1/cms/files/international-sites-desktop-4.png" style="max-width:100%" alt="Jelly Belly International Sites"></img>
							</a>
							<br>
						</div>
					{% endif %} 
					
 
                   <div class="clearfix"></div>
                    <!-- FOOTER 2ND COL -->
            </div>
        </div>
        {% endblock mz-footer-row2 %} 
      <div class="footer_copyright">
        <div class="mz-mobile">
          <p>Toll free: 1-800-522-3267</p> 
          &copy; 2011-<span>{% now Y %}</span> Jelly Belly Candy Company.<br />All Rights Reserved
        </div>
        <div class="mz-desktop">
              &copy; 2011-<span>{% now Y %}</span> Jelly Belly Candy Company. All Rights Reserved
        </div>
      </div>
    </div>
</footer>























