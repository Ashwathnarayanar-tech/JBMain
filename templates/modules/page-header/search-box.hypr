<form id="searchbox" class="mz-searchbox mz-l-formfieldgroup-cell" action="/search">
	<span id="search-instructions" class="sr-only">When typing in this field, a list of search results will appear and be automatically updated as you type.</span>
	<input aria-describedby="search-instructions" placeholder="{{ labels.search }}" type="search" class="mz-searchbox-field" value="{{pageContext.search.query}}" autocomplete="off" name="query" data-mz-role="searchquery" title="Search query field" aria-label="search" oninput="this.value=this.value.toLowerCase()"/>
 
    <input type="text" value="{{themeSettings.searchSort}}" name="sortBy" title="Sort-by input" style="display:none;"/><!-- ADA OK, field not shown -->
	<button class="mz-searchbox-button" type="submit" title="Search"><img alt="Search" src="data:image/png;base64," /></button>
	{% if pageContext.isDesktop or pageContext.isTablet %}
		<div class="clearfix"></div>
	{% else %}
		<div class="clearfix close-serch" tabindex="0" aria-label="close search bar"></div>
	{% endif %}
    {% require_script "modules/contextify" %} 
    {% require_script "modules/search-autocomplete" %}   
</form>
