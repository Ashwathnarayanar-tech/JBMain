										<div class="image-container"><img src="{{item.product.imageUrl}}" alt="{{item.product.name}}"/></div>
										<div class="item-detail">
											<span class="name">{{item.product.name|safe}}</span>
												{% with item.product.properties|findwhere('attributeFQN','tenant~IsHeatSensitive') as heat %}
				                                    {% for val in heat.values%}
				                                      {% if val.value   %}
				                                          <span class="heat" tabindex="0" role="contentinfo">heat-sensitive <span>i</span></span> 
				                                      {% endif %}
				                                    {% endfor %}
				                                {% endwith %}	
											 <div class="item-price-section">
											 	<span class="item-price">
											 		<label>Price</label>
										 			{% if item.product.price.price>item.product.price.salePrice %}<span class="saleprice">{{item.product.price.salePrice | currency}}</span>{% endif %}
													{% if item.product.price.price>item.product.price.salePrice %}<span class="strike">{{item.product.price.price | currency}}</span>{% else %}<span class="price">{{item.product.price.price |currency}}</span>{% endif %}
											 	</span>
											 	<span class="qty">
											 		<label>QTY</label>
											 		<span>{{item.quantity}}</span>
											 	</span>
											 	<span class="total">
											 		<label>Item Total</label>
											 		<span style="margin-left:10%">{% include "modules/common/item-total" %}</span>
											 	</span>
											 </div>
										</div>
									</div>
								{% endfor %}	
							</div>	
							<div class="subscription-sub-total">
								<div class="total-details">
									<div tabindex="0">
										<label>Sub Total</label><span>{{order.order.subtotal | currency}}</span>
									</div>
									{% if order.order.discountTotal !=0 %}
										<div tabindex="0">
											<label>Cart Discount</label><span class="discount">-{{order.order.discountTotal |currency}}</span>
										</div>
									 {% endif %}
									<div tabindex="0">
										<label>Tax</label><span>{{order.order.taxTotal | currency}}</span>
									</div>
									
									<div tabindex="0">
										<label>Shipping</label><span>{{order.order.shippingSubTotal | currency }}</span>
									</div>
								</div>	
								<div class="order-total" tabindex="0">
									<label>Order Total</label>
									<span>{{order.order.total|currency}}</span>
								</div>
							</div>
						</div>
						<span tabindex="0" role="button" {% if order.show %} aria-expanded="true" class="show-order-section show"{% else %} aria-expanded="false" class="show-order-section"{% endif %} data-mz-action="toggleOrderSummary" data-subscription-id="{{order.subscriptionId}}">Subscription Order Summary <span class="summary-arrow">{% if order.show %}<img src="../../../resources/images/top-arrow.svg" alt=""/>{% else %}<img src="../../../resources/images/down-arrow.svg" alt=""/>{% endif %}</span>
					</div>	
				</div>
	{% endwith %}
{% endif %}		
<div class="overlay-for-complete-page"></div>