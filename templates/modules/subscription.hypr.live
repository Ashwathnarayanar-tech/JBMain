			<input class="searchBoxSub" aria-label="select all items" type="text" placeholder="Search by Product Name, SKU Code etc." value="{{model.searchQuery}}" />
			<span class="search-icon"></span>
			{% if model.isSearchenabled %}
				{% if model.isDesktop %}
