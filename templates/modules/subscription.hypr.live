{% if model.isFirstPopup %}
	<div class="first-popup" >
		<div class="popup-body">
		
			<div class="popup-content" tabindex="0">
				<div class="head-sec" > 
					<h2 class="content-text" tabindex="0">Subscription Setup Page</h2>
				</div>
				<button class="close-icon" tabindex="0" role="button" data-mz-action="closePopup" aria-label="close popup"></button>
				<div class="body-sec">
					<ul class="item-list">  
						<li class="list-item first" >
							<img class="list-image" alt="You'll see your Subscription products here (and only here)" src="../../resources/images/popup-image-3.svg" />
							<span class="text-content-popup" tabindex="0">
								You'll see your Subscription products here <strong>(and only here)</strong>
							</span>
						</li> 	   
						<li class="list-item  second" >
							<img class="list-image" alt="When you are ready, click Create Subscription " src="../../resources/images/popup-image-2.svg" />
							<span class="text-content-popup" tabindex="0">
								When you are ready, click <strong>Create Subscription</strong>
							</span> 
						</li> 
						<li class="list-item third" >
							<img class="list-image" alt="We'll take you through a Special Checkout to confirm and start your Subscription" src="../../resources/images/popup-image-1.svg" />							
							<span class="text-content-popup" tabindex="0">
								We'll take you through a <strong>Special Checkout</strong> to confirm and start your Subscription
							</span>
						</li> 
					</ul>
				</div>
				{% if model.isMobile %}
				<div class="button-sec-gotIt" tabindex="0">
						<button class="gotIt"  tabindex="0" role="button" data-mz-action="closePopup" aria-label="Got it close popup">Got It</button>
					</div>
				{% endif %}
			</div>
		</div>
	</div>
{% endif %}
{% if model.popupData.isEnabled %}
    <div class="popup">
        <div class="popup-body">
            <div class="popup-content">
                <p class="message" tabindex="0"  role="text" aria-label = "{{model.popupData.adamessage}}">
                    {{model.popupData.message|safe}}
                </p>
				<button class="close-icon" tabindex="0" role="button" data-mz-action="closePopupdynamic" aria-label="close popup"></button>
                <div class="button-section">
                    {% for item in model.popupData.buttons %}
                        <button class="button button-{{item.class}}" data-mz-action="{{item.action}}">{{item.buttonLabel}}</button>
                    {% endfor %}
                </div>
            </div>
        </div> 
    </div>
{% endif %}
<div class="subscription-content ">
	{% if model.headBar  &&  model.isMobile %}
		<div class="top-bar" tabindex="0"> 
			<ul class="bar-list">
				<li class="bar-item" >
					<img class="list-image" alt="number one" src="../../resources/images/number-1.svg" />
					<span class="list-content" tabindex="0" >Find products and check the boxes to add your products</span>
				</li>
				<li class="bar-item">
					<img class="list-image" alt="number one" src="../../resources/images/number-3.svg" />
					<span class="list-content" tabindex="0" >Scroll down to review your Subscription List and Pick your cadence (how often)</span>
				</li>
				<li class="bar-item">
					<img class="list-image" alt="number one" src="../../resources/images/number-2.svg" />
					<span class="list-content" tabindex="0" >Create your Subscription and get your candy!</span>
				</li>
			</ul>
			<span class="close-icon" tabindex="0"  data-mz-action="removeBar" aria-label="close popup" ></span>
		</div>
	{% else %}
		<div class="top-bar" tabindex="0"> 
			<ul class="bar-list">
				<li class="bar-item" >
					<img class="list-image" alt="number one" src="../../resources/images/number-1.svg" />
					<span class="list-content" tabindex="0" >You'll see your Subscription products here (and only here)</span>
				</li>
				<li class="bar-item">
					<img class="list-image" alt="number one" src="../../resources/images/number-3.svg" />
					<span class="list-content" tabindex="0" >When you are ready, click Create Subscription</span>
				</li>
				<li class="bar-item">
					<img class="list-image" alt="number one" src="../../resources/images/number-2.svg" />
					<span class="list-content" tabindex="0" >We'll take you through a special Checkout to confirm and start your Subscription</span>
				</li>
			</ul>
			<span class="close-icon" tabindex="0"  data-mz-action="removeBar" aria-label="close popup" ></span>
		</div>
	{% endif %}
	<div class="header">
		<h2 class="page-tital" tabindex="0">Subscription Setup Page</h2>
		<span class="search-box-cointainer">
			<input class="searchBoxSub" aria-label="select all items" type="text" placeholder="Search by Product Name, SKU Code etc." value="{{model.searchQuery}}" />
			<span class="search-icon"></span>
			{% if model.isSearchenabled %}
				{% if model.isDesktop %}
					<div class="suggetion-list" tabindex="0">
