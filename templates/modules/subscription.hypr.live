							<span class="head-li description" tabindex="0">DESCRIPTION</span>
							<span class="head-li price" tabindex="0">PRICE</span>
							<span class="head-li heatsensitive" tabindex="0">HEAT-SENSITIVE <span class="heatIcon" > i </span></span>
							<span class="head-li quantity" tabindex="0">QUANTITY</span>					
						</div>
						{% endif %}
						<div class="suggetion-item-list" tabindex="0">
							{% if model.searchResult.length > 0 %}
								{% for item in model.searchResult %}
									<div class="suggetion-item" tabindex="0">
										<span class="item-li sku-input" tabindex="0">
											<span class="input-cointainer checkbox-outer-label">
												<input {% if not item.purchasableState.isPurchasable %}disabled="disabled"{% endif %} type="checkbox" name="selectitem" class="add-to-list-checkbox" {% if item.isSelected %}checked="checked"{% endif %} data-mz-attribute="{{item.productCode}}" data-mz-action="addtoList" />
												<span class="checkmark-style {% if not item.purchasableState.isPurchasable %}disabled{% endif %}" data-mz-attribute="{{item.productCode}}"></span>
											</span>
											<span class="sku-cointianer" tabindex="0" aria-label="product code is {{item.productCode}}">{{item.productCode}}</span>
										</span>
										<span class="item-li description-image" tabindex="0">
											<span class="image-cointainer">
												<img class="image-tag" src="{{item.image.imageUrl}}?max=80" alt="image for {{item.name|safe}}">
											</span>
											<span class="discription-text" tabindex="0">
												{{item.name|safe}}
											</span>
										</span> 
										<span class="item-li price">
