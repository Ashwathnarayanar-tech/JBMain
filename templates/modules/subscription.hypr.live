							{% if model.searchResult.length > 0 %}
								{% for item in model.searchResult %}
									<div class="suggetion-item" tabindex="0">
										<span class="item-li sku-input" tabindex="0">
											<span class="input-cointainer checkbox-outer-label">
												<input {% if not item.purchasableState.isPurchasable %}disabled="disabled"{% endif %} type="checkbox" name="selectitem" class="add-to-list-checkbox" {% if item.isSelected %}checked="checked"{% endif %} data-mz-attribute="{{item.productCode}}" data-mz-action="addtoList" />
												<span class="checkmark-style {% if not item.purchasableState.isPurchasable %}disabled{% endif %}" ></span>
											</span>
											<span class="sku-cointianer" tabindex="0" aria-label="product code is {{item.productCode}}">{{item.productCode}}</span>
										</span>
										<span class="item-li description-image" tabindex="0">
