	<div class="cat-prodList">
		{% if model.isDesktop || model.isTablet and !model.isPotreate %}
			<div class="side-bar">
				<div class="side-head">
					<span class="head-text" tabindex="0">Explore by <strong>Category</strong></span>
				</div>
				<div class="side-list">
					{% with model.categoryList|findwhere("isActive", true) as ActiveCategoryListProducts %}
						{% with model.subscriptionCategories.items as items %}
							{% for item in items %}
								<span class="mainCategory list-item side-bar-item" data-mz-attr="{{item.categoryId}}"> 
								<span tabindex="0" data-mz-attr="{{item.categoryId}}" class="mainCategory borderNone {% if model.activeCategoryList ==  item.categoryId %}active {% else %} {% if ActiveCategoryListProducts.Category.categoryCode == item.categoryId %} active {% endif %}{% endif %}" aria-label= "{{item.content.name}}">{{item.content.name|safe}} </span>
								<span class="icon close" tabindex="0" aria-label="Click Arrow to show sub categories" data-mz-attr="{{item.categoryId}}" ></span>
								<br> <a class="shopall" tabindex="0" aria-label="shop all" data-mz-attr="{{item.categoryId}}"> Shop All </a>
								
								</span> 
								<div class="subcategoryMainDiv" data-mz-categoryCode-code="{{item.categoryId}}">
