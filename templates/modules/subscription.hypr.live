{% if model.isFirstPopup %}
	<div class="first-popup">
		<div class="popup-body">
			<span class="close-icon" role="button" data-mz-action="closePopup" aria-label="close popup"></span>
			<div class="popup-content">
				<div class="head-sec"> 
					<h2 class="content-text">Subscription Setup Page</h2>
				</div>
				<div class="body-sec">
					<ul class="item-list">  
						<li class="list-item">
							<img class="list-image" alt="You'll see your Subscription products here (and only here)" src="../../resources/images/popup-image-3.svg" />
							<span class="text-content-popup" tabindex="0" role="contentinfo">
								You'll see your Subscription products here <strong>(and only here)</strong>
							</span>
						</li> 	   
						<li class="list-item">
							<img class="list-image" alt="We'll take you through a Special Checkout to confirm and start your Subscription" src="../../resources/images/popup-image-2.svg" />
							<span class="text-content-popup" tabindex="0" role="contentinfo">
								When you are ready, click <strong>Create Subscription</strong>
							</span> 
						</li> 
						<li class="list-item">
							<img class="list-image" alt="When you are ready, click Create Subscription" src="../../resources/images/popup-image-1.svg" />							
							<span class="text-content-popup" tabindex="0" role="contentinfo">
								We'll take you through a <strong>Special Checkout</strong> to confirm and start your Subscription
							</span>
						</li> 
					</ul>
				</div>
			</div>
		</div>
	</div>
{% endif %}
{% if model.popupData.isEnabled %}
    <div class="popup">
        <div class="popup-body">
            <div class="popup-content">
                <span class="message" tabindex="0" role="contentinfo">
                    {{model.popupData.message|safe}}
                </span>
				<button class="close-icon" tabindex="0" role="button" data-mz-action="closePopupdynamic" aria-label="close popup"></button>
                <div class="button-section">
                    {% for item in model.popupData.buttons %}
                        <button class="button button-{{item.class}}" data-mz-action="{{item.action}}">{{item.buttonLabel}}</button>
                    {% endfor %}
                </div>
            </div>
        </div> 
    </div>
{% endif %}
<div class="subscription-content {% if model.isMobileShowSummary %}showMobSummary{% endif %}">
	{% if model.headBar %}
		<div class="top-bar" tabindex="0"> 
			<ul class="bar-list">
				<li class="bar-item" >
					<img class="list-image" alt="number one" src="../../resources/images/number-1.svg" />
					<span class="list-content" tabindex="0" role="contentinfo">You'll see your Subscription products here (and only here)</span>
				</li>
				<li class="bar-item">
					<img class="list-image" alt="number one" src="../../resources/images/number-3.svg" />
					<span class="list-content" tabindex="0" role="contentinfo">When you are ready, click Create Subscription</span>
				</li>
				<li class="bar-item">
					<img class="list-image" alt="number one" src="../../resources/images/number-2.svg" />
					<span class="list-content" tabindex="0" role="contentinfo">We'll take you through a special Checkout to confirm and start your Subscription</span>
				</li>
			</ul>
			<span class="close-icon" tabindex="0"  data-mz-action="removeBar" aria-label="close popup" ></span>
		</div> 
	{% endif %}
